  cd "/Users/studiozo/Library/CloudStorage/GoogleDrive-zo@studiozonyc.com/My Drive/05 PRÃ˜JECTS/03 ZÃ˜ BÃ˜DYSUIT/07 EXÃ˜DUS x PIERRE /01 CODE/exodus-web-build/exodus_v1-main"

  # Pull latest from GitHub
  git pull origin main

  # See what you changed
  git status

  # Push your changes (3-step combo)
  git add .
  git commit -m "your message here"
  git push origin main

  # See recent commit history
  git log --oneline -5


